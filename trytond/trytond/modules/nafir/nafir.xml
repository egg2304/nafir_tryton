        </record>
        <record model="ir.action.act_window.view" id="act_window_view_ciudades_tree">
            <field name="sequence" eval="10"/>
            <field name="view" ref="view_ciudades_tree"/>
            <field name="act_window" ref="act_window_ciudades_form"/>
        </record>
